{
  "name": "automattic/sensei-certificates",
  "description": "Sensei LMS Certificates",
  "require-dev": {
    "php": "^7",
    "dealerdirect/phpcodesniffer-composer-installer": "0.7.1",
    "phpcompatibility/phpcompatibility-wp": "2.1.1",
    "squizlabs/php_codesniffer": "3.5.8",
    "wp-coding-standards/wpcs": "2.3.0"
  },
  "archive": {
    "exclude": [
      "/*",
      "!/admin",
      "!/assets/dist",
      "/assets/dist/css/*.js",
      "!/assets/images",
      "!/classes",
      "!/lang",
      "!/lib/tfpdf/tFPDF/*.php",
      "!/lib/tfpdf/font/*.php",
      "!/lib/tfpdf/font/unifont/*.php",
      "!/lib/tfpdf/font/unifont/*.ttf",
      "!/lib/tfpdf/font/unifont/*.dat",
      "!/templates",
      "!/changelog.txt",
      "!/LICENSE",
      "!/readme.txt",
      "!/widgets",
      "!/sensei-certificates-functions.php",
      "!/woothemes-sensei-certificates.php",
      ".DS_Store",
      ".*",
      "*.test.js"
    ]
  }
}
